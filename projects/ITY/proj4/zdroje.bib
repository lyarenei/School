@article{hole196,
author={Wexler,Joanie},
year={2010},
month={Jul 23},
title={WPA2 vulnerability found},
journal={Network World (Online)},
note={Název - AirTight Networks; Copyright - Copyright 2010 Network World, Inc. All Rights Reserved; Lidé - Ahmad, Sohail; Poslední aktualizace - 2016-03-14; SubjectsTermNotLitGenreText - Ahmad, Sohail},
abstract={Malicious insiders can exploit the vulnerability, named "Hole 196" by the researcher who discovered it at wireless security company AirTight Networks. The moniker refers to the page of the IEEE 802.11 Standard (Revision, 2007) on which the vulnerability is buried. WPA2 uses two types of keys: 1) Pairwise Transient Key (PTK), which is unique to each client, for protecting unicast traffic; and 2) Group Temporal Key (GTK) to protect broadcast data sent to multiple clients in a network. PTKs can detect address spoofing and data forgery. "GTKs do not have this property," according to page 196 of the IEEE 802.11 standard. "There's nothing in the standard to upgrade to in order to patch or fix the hole," says Kaustubh Phanse, AirTight's wireless architect who describes Hole 196 as a "zero-day vulnerability that creates a window of opportunity" for exploitation.},
keywords={Computers--Computer Networks; Ahmad, Sohail},
language={English},
url={https://search.proquest.com/docview/704395309?accountid=17115},
}